# For manual node placement you have to define the nodes positions for
# each and every node. We have created a separate file so not to clog
# the main omnetpp.ini and also to easily change the placement (simply include 
# another positions file in the main omnetpp.ini )

SN.node[*].phi = 0
SN.node[*].theta = 0

SN.node[0].xCoor = 20
SN.node[0].yCoor = 50
SN.node[0].zCoor = 0

SN.node[1].xCoor = 0
SN.node[1].yCoor = 50
SN.node[1].zCoor = 0

SN.node[2].xCoor = 22
SN.node[2].yCoor = 0
SN.node[2].zCoor = 0

SN.node[0].mobilityModuleName = "NoMobilityModule"
SN.node[1].mobilityModuleName = "NoMobilityModule"
SN.node[2].mobilityModuleName = "LineMobilityModule"

SN.node[*].nodeMobilityModule.printDebugInfo = true

SN.node[2].nodeMobilityModule.updateInterval = 100
SN.node[2].nodeMobilityModule.loc1_x = 22
SN.node[2].nodeMobilityModule.loc1_y = 0
SN.node[2].nodeMobilityModule.loc1_z = 0
SN.node[2].nodeMobilityModule.loc2_x = 22
SN.node[2].nodeMobilityModule.loc2_y = 100
SN.node[2].nodeMobilityModule.loc2_z = 0
SN.node[2].nodeMobilityModule.speed = 5

